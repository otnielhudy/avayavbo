<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#F8F8F8" />
        </shape>
    </item>


    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp">
        <bitmap
            android:src="@drawable/switch_camera"

            android:tint="#FFFF"
            />
    </item>


</layer-list>